{
  "rules": {
    "indent": [1, 2, { "SwitchCase": 1}],
    "quotes": [1, "single"],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"],
    "no-console": 0,
    "no-eval": 2,
    "no-implied-eval": 1,
    "eqeqeq": 2,
    "no-self-compare": 2,
    "no-new-wrappers": 2,
    "no-new-func": 2,
    "no-fallthrough": 2,
    "no-warning-comments": [1, {
        "terms": ["todo", "hack", "fixme", "evil"],
        "location": "start"
    }],
    "no-void": 2,
    "wrap-iife": [1, "outside"],
    "comma-spacing": [1, {"before": false, "after": true}],
    "no-with": 2,
    "no-shadow": 1,
    "no-undef-init": 1,
    "no-undefined": 1,
    "new-cap": [2, {"capIsNew": false, "newIsCap": true}],
    "key-spacing": [1, {"beforeColon": false, "afterColon": true, "align": "value"}],
    "consistent-this": [2, "self"],
    "comma-style": [2, "last"],
    "no-spaced-func": 1,
    "no-lonely-if": 2,
    "no-useless-call": 1,
    "space-infix-ops": 1,
    "no-array-constructor": 2,
    "no-new-object": 2,
    "no-continue": 2,
    "no-unexpected-multiline": 2,
    "no-extra-semi": 1,
    "semi-spacing": [1, {"before": false, "after": true}],
    "space-before-function-paren": [1, "never"],
    "space-in-parens": [1, "never"],
    "no-useless-concat": 1,
    "no-sequences": 2,
    "no-else-return": 2,
    "no-shadow-restricted-names": 1,
    "no-use-before-define": [2, "nofunc"],
    "default-case": 0,
    "no-path-concat": 2,
    "quote-props": [1, "consistent-as-needed"],
    "no-negated-condition": 1,
    "keyword-spacing": 1,
    "max-len": [1, 100, 2, {"ignoreUrls": true}],
    "array-bracket-spacing": [1, "never"],
    "block-spacing": [1, "always"],
    "no-mixed-spaces-and-tabs": [1, "smart-tabs"],
    "no-unneeded-ternary": 1,
    "one-var": [2, "never"],
    "eol-last": 1,
    "operator-linebreak": [1, "after", { "overrides": { "?": "before", ":": "before" } }],
    "no-obj-calls": 2,
    "no-cond-assign": [2, "always"],
    "no-empty-character-class": 2,
    "no-invalid-regexp": 2,
    "no-duplicate-case": 2,
    "no-dupe-keys": 2,
    "no-dupe-args": 2,
    "no-empty": 1,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": 0,
    "use-isnan": 2,
    "valid-typeof": 2,
    "no-unused-vars": 1,
    "max-lines": [1, { "max": 300, "skipComments": true, "skipBlankLines": true }],
    "max-params": [1, 4],
    "newline-per-chained-call": 1,
    "no-bitwise": 2,
    "no-inline-comments": 0,
    "no-mixed-operators": 2,
    "no-plusplus": [2, { "allowForLoopAfterthoughts": true }],
    "no-nested-ternary": 1,
    "no-restricted-syntax": [2, "WithStatement"],
    "no-alert": 1,
    "comma-dangle": 1
  },
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true,
    "experimentalDecorators": true,
    "generators": true,
    "arrowFunctions": true,
    "defaultParams": true,
    "spread": true,
    "restParams": true,
    "blockBindings": true,
    "templateStrings": true,
    "destructuring": true,
    "forOf": true,
    "regexUFlag": true,
    "regexYFlag": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralShorthandProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralDuplicateProperties": true,
    "classes": true
  },
  "plugins": [
    "react"
  ]
}
